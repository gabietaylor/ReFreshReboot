        "react-native-calendar": "^0.13.1",
        "react-native-form-generator": "^0.9.9",
        "react-native-lock": "^0.6.1",
        "react-native-material-kit": "^0.5.1",
        "react-native-vector-icons": "^4.4.3",
        "react-redux": "^5.0.6",
