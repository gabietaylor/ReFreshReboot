        "react-native-scripts": "1.8.1",
        "react-test-renderer": "16.0.0",
        "rimraf": "^2.6.2",
        "sinon": "^4.1.3"
    },
    "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
    "scripts": {
        "start": "react-native-scripts start",
        "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.ios.js --bundle-output ios/backScratch/main.jsbundle --dev=false --minify --verbose",
        "eject": "react-native-scripts eject",
        "android": "react-native-scripts android",
        "ios": "react-native-scripts ios",
        "test": "node node_modules/jest/bin/jest.js --watch",
        "test:watch": "chokidar 'src/**/*.js' 'test/**/*.js' -c 'npm test'",
        "lint": "eslint src test",
        "coverage": "rimraf coverage && istanbul cover --i=src/**/*.js _mocha -- src/**/*.spec.js",
        "check-coverage": "istanbul check-coverage coverage/coverage.json"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "dependencies": {
        "bluebird": "^3.5.1",
        "es6-symbol": "^3.1.1",
