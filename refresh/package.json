        "babel-preset-react-native": "^4.0.0",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "chai": "^4.1.2",
        "chokidar-cli": "^1.2.0",
        "enzyme": "^3.3.0",
        "eslint": "^4.14.0",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-react": "^7.5.1",
        "fetch-mock": "^6.0.0-beta.3",
        "istanbul": "^0.4.5",
        "jest-expo": "23.0.0",
        "mocha": "^4.1.0",
        "node-fetch": "^1.7.3",
        "react-addons-test-utils": "^15.6.2",
